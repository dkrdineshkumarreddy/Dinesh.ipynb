      "source": [
        "password=input(\"Enter password : \")\n",
        "length=len(password)\n",
        "lower_count=0\n",
        "upper_count=0\n",
        "digit_count=0\n",
        "symbol_count=0\n",
        "for i in password:\n",
        "    if i.islower():\n",
        "         lower_count +=1\n",
        "    elif i.isupper():\n",
        "         upper_count +=1\n",
        "    elif i.isdigit():\n",
        "         digit_count +=1\n",
        "    else:\n",
        "         symbol_count +=1\n",
        "if length>=8 and length<=14 and lower_count>0 and upper_count>0 and digit_count>0 and symbol_count>0:\n",
        "     print(\"lower count is : \",lower_count)\n",
        "     print(\"upper count is : \",upper_count)\n",
        "     print(\"digit count is : \",digit_count)\n",
        "     print(\"symbol count is : \",symbol_count)\n",
        "     print(\"user password is valid\")\n",
        "else:\n",
        "     print(\"password is not valid.\")\n"
      ],
